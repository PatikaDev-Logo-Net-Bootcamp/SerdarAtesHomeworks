@using ApartmentManagement.Business.DTOs
@model List<BillDto>
@{
    ViewData["Title"] = "GetUserPaidBills";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Ödenmiş Faturalar</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Fiyat
            </th>
            <th>
                Daire Numarası
            </th>
            <th>
                Fatura Tarihi
            </th>
            <th>
                Fatura türü
            </th>
            <th>
         
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FlatNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BillDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BillTypeName)
            </td>
       
        </tr>
}
    </tbody>
</table>
