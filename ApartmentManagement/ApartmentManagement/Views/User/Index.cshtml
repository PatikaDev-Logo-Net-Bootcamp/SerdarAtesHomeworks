@using ApartmentManagement.Business.DTOs
@model List<UserDto>

<table class="table">
    <thead>
        <tr>
             <th>
                <label>FirstName</label>
            </th>
            <th>
            <label>LastName</label>
            </th>
            <th>
              <label>Email</label>
            </th>
            <th>
               <label>Plaka</label>
            </th>
            <th>
               <label>Tc</label>
            </th>
            <th></th>
        </tr>
    </thead>
<tbody>
    @foreach (var item in Model) {
        <tr>
          
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.CarPlateNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TcNo)
            </td>

            <td>
                <a asp-action="UpdateUser" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="DeleteUser" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>